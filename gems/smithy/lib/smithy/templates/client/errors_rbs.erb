module <%= namespace %>
  module Errors
<% errors.each do |error| -%>
    class <%= error.name %> < Smithy::Client::Errors::ServiceError
<% error.members.each do |member| -%>
      def <%= member.name %>: () -> <%= Model::Rbs.type(model, member.shape['target'], Model.shape(model, member.shape['target'])) %>?
<% end -%>
<% if error.retryable? -%>
      def retryable?: () -> true
<% end -%>
<% if error.throttling? -%>
      def throttling?: () -> true
<% end -%>
    end
<% end -%>
  end
end
