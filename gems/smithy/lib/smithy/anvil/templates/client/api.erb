# frozen_string_literal: true

# This is generated code!

module <%= namespace %>
  module Api
    include Smithy::Client::Shapes

<%# Shapes definition -%>
<%- shapes.each do |shape| -%>
    <%= shape.name %> = <%= shape.type %>.new(
      shape_id: "<%= shape.id %>",
    <%- if shape.typed -%>
      type: Types::<%= shape.name %>,
    <%- end -%>
    <%- unless shape.traits.empty? -%>
      traits: <%= shape.traits %>
    <%- end -%>
    )
<%- end %>
<%# Adding members to Shapes -%>
<%- shapes_with_members.each do |shape| -%>
    <%- shape.members.each do |member| -%>
    <%= shape.name %>.add_member(
      "<%= member.name %>",
      <%= member.shape %>,
    <%- unless member.traits.empty? -%>
      traits: <%= member.traits %>
    <%- end -%>
    )
    <%- end %>
<%- end -%>

  end
end
