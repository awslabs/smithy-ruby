module Smithy
  module Client
    module HTTP
      class Headers
        include Enumerable[[String,String]]

        def []: (String | Symbol) -> String
        def []=: (String | Symbol, String) -> void
        def update: (Headers | Hash[String, String]) -> Headers
        def delete: (String) -> String?
        def clear: () -> Headers
        def empty?: () -> bool
        def keys: () -> Array[String]
        def values: () -> Array[String]
        def values_at: (Array[String]) -> Array[String]
        def key?: (String) -> bool
        def to_hash: () -> Hash[String, String]
      end
    end
  end
end