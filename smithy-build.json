{
    "version": "1.0",
    "maven": {
        "dependencies": [
            "software.amazon.smithy:smithy-protocol-traits:1.54.0"
        ]
    },
    "projections": {
        "weather": {
            "plugins": {
                "run::smithy-ruby": {
                    "command": ["smithy-ruby", "smith", "client", "--gem-name=weather", "--gem-version=1.0.0", "--destination-root=../../../../projections/weather"]
                }
            }
        }
    }
}
